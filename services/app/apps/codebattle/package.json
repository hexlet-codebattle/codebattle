      "^@/components(.*)$": "<rootDir>/assets/js/widgets/components$1",
      "^@/lib(.*)$": "<rootDir>/assets/js/widgets/lib$1",
      "^@/machines(.*)$": "<rootDir>/assets/js/widgets/machines$1",
      "^@/middlewares(.*)$": "<rootDir>/assets/js/widgets/middlewares$1",
