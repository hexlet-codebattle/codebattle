      "^@/slices(.*)$": "<rootDir>/assets/js/widgets/slices$1",
      "^@/utils(.*)$": "<rootDir>/assets/js/widgets/utils$1"
    },
    "testPathIgnorePatterns": [
      "/helpers/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
